<!doctype html>
<html lang="en">
    <head>
        <title>IoF Main Page</title>
        <% include ../../../../partials/customer/MainPage/afterstylesheet.ejs %>
        
    </head>
    <body>
        <!-- header -->
        <% include ../../../../partials/customer/MainPage/afterMainHeader.ejs %>
        <!-- end header -->
        <div class="wrapper">
            <div class="content-wrapper">
                <div class="content_menu">
                    <h1>Dashboard</h1>
                    <div class="showButtons">
                        <button class="showButton1" type="button"></button>
                        <button class="showButton2" type="button"></button>
                        <button class="showButton3" type="button"></button>
                        <button class="addPanel btn-primary">Add Panel to Dashboard</button>
                    </div>
                </div>
                
            </div>
        </div>
            <!-- Footer -->
        <% include ../../../../partials/customer/MainPage/footer.ejs %>
        <% include ../../../../partials/customer/MainPage/afterjavascript.ejs %>
        
        <script>
            $(document).ready(function(){
               
                /* Layout Fix */
                function fix()
                {
                    var window_height = $(window).height();
                    var footer_height = $(".main-footer").outerHeight() || 0;
                    $('body, html, .wrapper').css('height', 'auto');
                    $(".content-wrapper").css("min-height", window_height - footer_height);
                }
                fix();
                $(window, ".wrapper").resize(function(){
                    fix();
                });
            });
    </script>
        
    </body>
</html>