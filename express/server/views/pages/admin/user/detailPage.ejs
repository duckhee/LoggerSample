<!DOCTYPE html>
<html>
  <head>
    <title>Main</title>
    <% include ../../../partials/admin/stylesheet.ejs %>
  </head>
  
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="/" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>C</b>&amp;H</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>C &amp; H </b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                 <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                        <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Tasks: style can be found in dropdown.less -->
                        <!-- header nav 들어가는 부분 -->
                        <% include ../../../partials/admin/headerNav.ejs %>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="/static/images/logo_iot.png" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        
                        <!-- <a href="#"><i class="fa fa-circle text-success"></i> Online</a> -->
                    </div>
                </div>
                <!-- nav item -->
                <% include ../../../partials/admin/navItem.ejs %>
                <!-- end nav item -->
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
                <!-- Content Header (Page header) -->
                 <section class="content-header">
                    <h1>
                        Member <small>update Page</small>
                    </h1>
                    <ol class="breadcrumb">
                        <li><a href="/"><i class="fa fa-users"></i> Home</a></li>
                        <li><i class="ffa fa-dshboard"></i>Member</li>
                        <li class="active"><a href="#">Detail</a></li>
                    </ol>
                </section> 
            <div class="content">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="box">
                            <div class='box-header'>
                                <h3 class="box-title"><i class="fa fa-fw fa-user"></i>Member Detail(<%=user.userId%>)<h3>
                            </div>
                            <!--./box-header end-->
                            
                            <div class="box-body">
                            <form class="form-horizontal">
                                <div class="form-group col-sm-offset-2">
                                    <label for="userID" class="col-sm-2 control-label">User ID</label>
                                    <div class="col-sm-10">
                                        <span class="form-control"><%=user.userId%></span>
                                    </div>
                                </div>
                                <!--./user id end -->
                                <div class="form-group col-sm-offset-2">
                                    <label for="userID" class="col-sm-2 control-label">User Name</label>
                                    <div class="col-sm-10">
                                        <span class="form-control"><%=user.userName%></span>
                                    </div>
                                </div>
                                <!--./user name end-->
                                <div class="form-group col-sm-offset-2">
                                    <label for="userID" class="col-sm-2 control-label">Registe Date</label>
                                    <div class="col-sm-10">
                                    <%
                                    var CreateDate = new Date(user.createdAt);
                                    var RegisteDate = CreateDate.getFullYear()+"년 "+(CreateDate.getMonth()+1)+"월 "+CreateDate.getDate()+"일 "+CreateDate.getHours()+"시";
                                    %>
                                        <span class="form-control"><%=RegisteDate%></span>
                                    </div>
                                </div>
                                <!--./user registe date end -->
                               
                                <%for(var i =0;i <  user.loggerInfos.length;i++){%>
                               <div class="form-group">
                                    <label for="loggerInfo" class="col-sm-2 control-label"><h3 class="box-title"><i class="fa fa-fw fa-database"></i><%=user.loggerInfos[i].LoggerFolderName%></h3></label>
                                </div> 
                                <!--./Logger info-->
                                <div class="form-group col-sm-offset-2">
                                    <label for="loggerInfo" class="col-sm-2 control-label">Logger Name</label>
                                    <div class="col-sm-10">
                                        <span class="form-control"><%=user.loggerInfos[i].LoggerFolderName%></span>
                                    </div>
                                </div>
                                <!--./user name end-->
                                <div class="form-group col-sm-offset-2">
                                    <label for="loggerInfoFileType" class="col-sm-2 control-label">FILE TYPE</label>
                                    <div class="col-sm-10">
                                        <span class="form-control"><%=user.loggerInfos[i].LoggerKind%></span>
                                    </div>
                                </div>
                                <%}%>
                            </form>
                            </div>
                            <!--./box-body end-->
                            <div class="box-footer">
                                 
                            </div>
                            <!--./box-footer end -->
                        </div>
                        <!--./box end -->
                    </div>
                    <!--./col-xs-12 end -->
                </div>
                <!--./row end -->
            
            </div>
        </div>
    <% include ../../../partials/admin/footer.ejs %>
    <% include ../../../partials/admin/footerScript.ejs %>
  </body>
</html>